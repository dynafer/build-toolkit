{
  "name": "@dynafer/build-toolkit",
  "version": "1.0.63",
  "repository": "https://github.com/dynafer/build-toolkit.git",
  "author": "Dynafer",
  "bin": {
    "build-toolkit": "bin/build-toolkit.js"
  },
  "scripts": {
    "prepublishOnly": "yarn run lint & yarn run build",
    "build:commonjs": "tsc -p ./tsconfig.commonjs.json",
    "build:module": "tsc -p ./tsconfig.module.json",
    "build:types": "tsc -p ./tsconfig.types.json --emitDeclarationOnly",
    "build": "node build.js",
    "watch": "node build.js -w",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "devDependencies": {
    "@finer-group/eslint-plugin": "^0.0.9",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.1",
    "@types/uglify-js": "^3.17.1",
    "eslint": "^8.40.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-environment-node-single-context": "^29.1.0",
    "jest-silent-reporter": "^0.5.0",
    "rollup": "^3.21.5",
    "sass": "^1.62.1",
    "uglify-js": "^3.17.4"
  },
  "main": "build/lib/BuildToolkit.js",
  "module": "build/lib/module/BuildToolkit.js",
  "types": "build/lib/module/BuildToolkit.d.ts",
  "exports": {
    "import": "./build/lib/module/BuildToolkit.js",
    "require": "./build/lib/BuildToolkit.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "dynafer",
    "build-toolkit",
    "build",
    "toolkit",
    "tool",
    "rollup",
    "sass",
    "command",
    "task",
    "runner",
    "jest",
    "test"
  ],
  "license": "MIT"
}
